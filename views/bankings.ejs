<!DOCTYPE html>
<html>
    <head>
        <% include ./layouts/pro/head %>
    </head>
    <body>
        <div class="wrapper">
            <% include ./layouts/pro/sidebar %>
            <div class="main-panel">
                <% include ./layouts/pro/navbar %>

                <div class="main-content">
                    <div class="container-fluid">


                        <div class="row">


                            <% if(user.accessLevel.banking == 10) { %>
                                <div class="col-md-12">
                                    <div class="card">
                                        <div class="header">
                                            <h4 class="title">My Bankings</h4>
                                        </div>
                                        <div class="content">
                                            <div class="dropdown">

                                                <button class="btn dropdown-toggle" type="button" id="dropdownMenu1" data-toggle="dropdown">
                                                    Show
                                                    <span class="caret"></span>
                                                </button>

                                                <ul class="dropdown-menu" role="menu" aria-labelledby="dropdownMenu1">
                                                    <li role="presentation"><a role="menuitem" tabindex="-1" href="/banking-m/myprofile/today">Today</a></li>
                                                    <li role="presentation" class="divider"></li>
                                                    <li role="presentation"><a role="menuitem" tabindex="-1" href="/banking-m/myprofile/all">All</a></li>
                                                </ul>

                                            </div>
                                        </div>
                                    </div>
                                </div>
                            <% } %>

                            <% if(user.accessLevel.banking == 9) { %>
                                <div class="col-md-12">
                                    <div class="card">
                                        <div class="header">
                                            <h4 class="title">My Area Profile</h4>
                                        </div>
                                        <div class="content">
                                            <div class="dropdown">

                                                <button class="btn dropdown-toggle" type="button" id="dropdownMenu1" data-toggle="dropdown">
                                                    Show
                                                    <span class="caret"></span>
                                                </button>

                                                <ul class="dropdown-menu" role="menu" aria-labelledby="dropdownMenu1">
                                                    <li role="presentation"><a role="menuitem" tabindex="-1" href="/banking-a/areaprofile/today">Today</a></li>
                                                    <li role="presentation" class="divider"></li>
                                                    <li role="presentation"><a role="menuitem" tabindex="-1" href="/banking-a/areaprofile/all">All</a></li>
                                                </ul>

                                            </div>
                                        </div>
                                    </div>
                                </div>
                            <% } %>

                            <% if(user.accessLevel.banking == 8) { %>
                                <div class="col-md-12">
                                    <div class="card">
                                        <div class="header">
                                            <h4 class="title">Bankings</h4>
                                        </div>
                                        <div class="content">
                                            <div class="dropdown">
                                                <button class="btn dropdown-toggle" type="button" id="dropdownMenu1" data-toggle="dropdown">
                                                    Show
                                                    <span class="caret"></span>
                                                </button>

                                                <ul class="dropdown-menu" role="menu" aria-labelledby="dropdownMenu1">
                                                    <li role="presentation"><a role="menuitem" tabindex="-1" href="/banking/today">Today</a></li>
                                                    <li role="presentation" class="divider"></li>
                                                    <li role="presentation"><a role="menuitem" tabindex="-1" href="/banking/all">All</a></li>
                                                </ul>
                                            </div>
                                            <p class="title">Search</p>
                                            <form action="/banking/date" method="get">
            									<div class="form-group">
            										<input class="form-control" type="text" name="date" placeholder="YYYY-MM-DD">
            									</div>
            									<div class="form-group">
            										<input class="btn btn-primary" type="submit" value="List">
            									</div>
            								</form>
                                        </div>
                                    </div>
                                </div>
                            <% } %>

                        </div>



                    </div>
                </div>

                <% include ./layouts/pro/footer %>
            </div>
        </div>
    </body>
    <% include ./layouts/pro/includes %>
</html>
