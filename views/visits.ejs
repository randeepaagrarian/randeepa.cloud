<!DOCTYPE html>
<html>
    <head>
        <% include ./layouts/pro/head %>
    </head>
    <body>
        <div class="wrapper">
            <% include ./layouts/pro/sidebar %>
            <div class="main-panel">
                <% include ./layouts/pro/navbar %>

                <div class="main-content">
                    <div class="container-fluid">


                        <div class="row">

                            <div class="col-md-12">
                                <div class="card">
                                    <div class="container-fluid">
                                        <% if(user.accessLevel.visits == 8 || user.accessLevel.visits == 9 || user.accessLevel.visits == 10 || user.accessLevel.visits == 11 || user.accessLevel.visits == 12) { %>
                                            <div class="row">
                                                <div class="header">
                                                    <h4 class="title">My Visits</h4>
                                                </div>
                                            </div>
                                            <div class="row">
                                                <div class="col-md-6">
                                                    <p class="title">Field Visits</p>
                                                    <div class="content">
                                                        <div class="dropdown">

                                                            <button class="btn dropdown-toggle" type="button" id="dropdownMenu1" data-toggle="dropdown">
                                                                Show
                                                                <span class="caret"></span>
                                                            </button>

                                                            <ul class="dropdown-menu" role="menu" aria-labelledby="dropdownMenu1">
                                                                <li role="presentation"><a role="menuitem" tabindex="-1" href="/visit/myprofile/fieldvisit/today">Today</a></li>
                                                                <li role="presentation"><a role="menuitem" tabindex="-1" href="/visit/myprofile/fieldvisit/all">All</a></li>
                                                            </ul>

                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="col-md-6">
                                                    <p class="title">Organizational Visits</p>
                                                    <div class="content">
                                                        <div class="dropdown">

                                                            <button class="btn dropdown-toggle" type="button" id="dropdownMenu1" data-toggle="dropdown">
                                                                Show
                                                                <span class="caret"></span>
                                                            </button>

                                                            <ul class="dropdown-menu" role="menu" aria-labelledby="dropdownMenu1">
                                                                <li role="presentation"><a role="menuitem" tabindex="-1" href="/visit/myprofile/organizationalvisit/today">Today</a></li>
                                                                <li role="presentation"><a role="menuitem" tabindex="-1" href="/visit/myprofile/organizationalvisit/all">All</a></li>
                                                            </ul>

                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        <% } %>

                                        <% if(user.accessLevel.visits == 8) { %>
                                            <div class="row">
                                                <div class="header">
                                                    <h4 class="title">All Visits</h4>
                                                </div>
                                            </div>
                                            <div class="row">
                                                <div class="col-md-6">
                                                    <p class="title">Field Visits</p>
                                                    <div class="content">
                                                        <div class="dropdown">

                                                            <button class="btn dropdown-toggle" type="button" id="dropdownMenu1" data-toggle="dropdown">
                                                                Show
                                                                <span class="caret"></span>
                                                            </button>

                                                            <ul class="dropdown-menu" role="menu" aria-labelledby="dropdownMenu1">
                                                                <li role="presentation"><a role="menuitem" tabindex="-1" href="/visit/fieldvisit/today">Today</a></li>
                                                                <li role="presentation"><a role="menuitem" tabindex="-1" href="/visit/fieldvisit/all">All</a></li>
                                                            </ul>

                                                        </div>

                                                        <h5 class="category">By Date All</h5>
                                                        <br>
                                                        <form method="get" action="/visit/fieldvisit/bydate">
                                                            <input type="text" name="date" class="form-control datetimepicker" placeholder="YYYY-MM-DD" />
                                                            <br>
                                                            <div class="form-group">
                                                                <input class="btn btn-primary" type="submit" value="List">
                                                            </div>
                                                        </form>

                                                        <h5 class="category">By Date Officer</h5>
                                                        <br>
                                                        <form method="get" action="/visit/fieldvisit/officer/bydate">
                                                            <select class="form-control" name="officer">
                                                                <% users.forEach(function(user) { %>
                                                                    <option value="<%= user.id %>"><%= user.name %></option>
                                                                <% }) %>
                                                            </select>
                                                            <br>
                                                            <input type="text" name="date" class="form-control datetimepicker" placeholder="YYYY-MM-DD" />
                                                            <br>
                                                            <div class="form-group">
                                                                <input class="btn btn-primary" type="submit" value="List">
                                                            </div>
                                                        </form>
                                                    </div>
                                                </div>
                                                <div class="col-md-6">
                                                    <p class="title">Organizational Visits</p>
                                                    <div class="content">
                                                        <div class="dropdown">

                                                            <button class="btn dropdown-toggle" type="button" id="dropdownMenu1" data-toggle="dropdown">
                                                                Show
                                                                <span class="caret"></span>
                                                            </button>

                                                            <ul class="dropdown-menu" role="menu" aria-labelledby="dropdownMenu1">
                                                                <li role="presentation"><a role="menuitem" tabindex="-1" href="/visit/organizationalvisit/today">Today</a></li>
                                                                <li role="presentation"><a role="menuitem" tabindex="-1" href="/visit/organizationalvisit/all?page=1">All</a></li>
                                                            </ul>

                                                        </div>

                                                        <h5 class="category">By Date All</h5>
                                                        <br>
                                                        <form method="get" action="/visit/organizationalvisit/bydate">
                                                            <input type="text" name="date" class="form-control datetimepicker" placeholder="YYYY-MM-DD" />
                                                            <br>
                                                            <div class="form-group">
                                                                <input class="btn btn-primary" type="submit" value="List">
                                                            </div>
                                                        </form>

                                                        <h5 class="category">By Date Officer</h5>
                                                        <br>
                                                        <form method="get" action="/visit/organizationalvisit/officer/bydate">
                                                            <select class="form-control" name="officer">
                                                                <% users.forEach(function(user) { %>
                                                                    <option value="<%= user.id %>"><%= user.name %></option>
                                                                <% }) %>
                                                            </select>
                                                            <br>
                                                            <input type="text" name="date" class="form-control datetimepicker" placeholder="YYYY-MM-DD" />
                                                            <br>
                                                            <div class="form-group">
                                                                <input class="btn btn-primary" type="submit" value="List">
                                                            </div>
                                                        </form>
                                                    </div>
                                                </div>
                                            </div>
                                        <% } %>

                                        <% if(user.accessLevel.visits == 9) { %>
                                            <div class="row">
                                                <div class="header">
                                                    <h4 class="title">Area Visits</h4>
                                                </div>
                                            </div>
                                            <div class="row">
                                                <div class="col-md-6">
                                                    <p class="title">Field Visits</p>
                                                    <div class="content">
                                                        <div class="dropdown">

                                                            <button class="btn dropdown-toggle" type="button" id="dropdownMenu1" data-toggle="dropdown">
                                                                Show
                                                                <span class="caret"></span>
                                                            </button>

                                                            <ul class="dropdown-menu" role="menu" aria-labelledby="dropdownMenu1">
                                                                <li role="presentation"><a role="menuitem" tabindex="-1" href="/visit/areaprofile/fieldvisit/today">Today</a></li>
                                                                <li role="presentation"><a role="menuitem" tabindex="-1" href="/visit/areaprofile/fieldvisit/all">All</a></li>
                                                            </ul>

                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="col-md-6">
                                                    <p class="title">Organizational Visits</p>
                                                    <div class="content">
                                                        <div class="dropdown">

                                                            <button class="btn dropdown-toggle" type="button" id="dropdownMenu1" data-toggle="dropdown">
                                                                Show
                                                                <span class="caret"></span>
                                                            </button>

                                                            <ul class="dropdown-menu" role="menu" aria-labelledby="dropdownMenu1">
                                                                <li role="presentation"><a role="menuitem" tabindex="-1" href="/visit/areaprofile/organizationalvisit/today">Today</a></li>
                                                                <li role="presentation"><a role="menuitem" tabindex="-1" href="/visit/areaprofile/organizationalvisit/all">All</a></li>
                                                            </ul>

                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        <% } %>
                                    </div>
                                </div>
                            </div>

                        </div>



                    </div>
                </div>

                <% include ./layouts/pro/footer %>
            </div>
        </div>
    </body>
    <% include ./layouts/pro/includes %>
    <script type="text/javascript">
        $('.datetimepicker').datetimepicker({
            format: 'YYYY-MM-DD',
            icons: {
                time: "fa fa-clock-o",
                date: "fa fa-calendar",
                up: "fa fa-chevron-up",
                down: "fa fa-chevron-down",
                previous: 'fa fa-chevron-left',
                next: 'fa fa-chevron-right',
                today: 'fa fa-screenshot',
                clear: 'fa fa-trash',
                close: 'fa fa-remove'
            }
        });
    </script>
</html>
