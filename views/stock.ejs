<!DOCTYPE html>
<html>
    <head>
        <% include ./layouts/pro/head %>
        <style media="screen">
            .ct-label.ct-horizontal { position: relative; transform: rotate(90deg); transform-origin: left top; }
        </style>
    </head>
    <body>
        <div class="wrapper">
            <% include ./layouts/pro/sidebar %>
            <div class="main-panel">
                <% include ./layouts/pro/navbar %>

                <div class="main-content">
                    <div class="container-fluid">


                        <div class="row">

                            <% if(user.accessLevel.stock == 8) { %>

                                <div class="col-md-12">
                                    <div class="card">
                                        <div class="header">
                                            <h4 class="title"><%= title %></h4>
                                        </div>
                                        <div class="container-fluid">
                                            <div class="row">
                                                <div class="col-md-6">
                                                    <div class="row">
                                                        <div class="col-md-6">
                                                            <h5 class="title">Delivery Documents</h5>
                                                            <div class="content">
                                                                <div class="dropdown">

                                                                    <button class="btn dropdown-toggle" type="button" id="dropdownMenu1" data-toggle="dropdown">
                                                                        Show
                                                                        <span class="caret"></span>
                                                                    </button>

                                                                    <ul class="dropdown-menu" role="menu" aria-labelledby="dropdownMenu1">
                                                                        <li role="presentation"><a role="menuitem" tabindex="-1" href="#">Issue</a></li>
                                                                        <li role="presentation" class="divider"></li>
                                                                        <li role="presentation"><a role="menuitem" tabindex="-1" href="/stock/packingList">Packing List</a></li>
                                                                        <li role="presentation"><a role="menuitem" tabindex="-1" href="/stock/deliveryNote">Delivery Note</a></li>
                                                                        <li role="presentation"><a role="menuitem" tabindex="-1" href="/stock/transferNote">Transfer Note</a></li>
                                                                        <li role="presentation"><a role="menuitem" tabindex="-1" href="/stock/returnNote">Return Note</a></li>
                                                                        <li role="presentation" class="divider"></li>
                                                                        <li role="presentation"><a role="menuitem" tabindex="-1" href="/stock/allDeliveryDocuments?page=1">View All</a></li>
                                                                    </ul>

                                                                </div>
                                                            </div>
                                                        </div>
                                                        <div class="col-md-6">
                                                            <h5 class="title">Stock</h5>
                                                            <div class="content">
                                                                <div class="dropdown">

                                                                    <button class="btn dropdown-toggle" type="button" id="dropdownMenu1" data-toggle="dropdown">
                                                                        Show
                                                                        <span class="caret"></span>
                                                                    </button>

                                                                    <ul class="dropdown-menu" role="menu" aria-labelledby="dropdownMenu1">
                                                                        <li role="presentation"><a role="menuitem" tabindex="-1" href="/stock/stock">View Stock</a></li>
                                                                    </ul>

                                                                </div>
                                                            </div>
                                                        </div>
                                                        <div class="col-md-6">
                                                            <h5 class="title">Dealer</h5>
                                                            <div class="content">
                                                                <div class="dropdown">

                                                                    <button class="btn dropdown-toggle" type="button" id="dropdownMenu1" data-toggle="dropdown">
                                                                        Show
                                                                        <span class="caret"></span>
                                                                    </button>

                                                                    <ul class="dropdown-menu" role="menu" aria-labelledby="dropdownMenu1">
                                                                        <li role="presentation"><a role="menuitem" tabindex="-1" href="/stock/dealer/all">All</a></li>
                                                                        <li role="presentation"><a role="menuitem" tabindex="-1" href="/stock/dealer/add">Add</a></li>
                                                                    </ul>

                                                                </div>
                                                            </div>
                                                        </div>
                                                        <div class="col-md-12">
                                                            <h5 class="title">Search</h5>
                                                            <div class="content">
                                                                <form action="/stock/search" method="get">
                                									<div class="form-group">
                                										<input class="form-control" type="text" name="skw" placeholder="Search Keyword">
                                									</div>
                                									<div class="form-group">
                                										<input class="btn btn-default" type="submit" value="Search">
                                									</div>
                                								</form>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="col-md-6">
                                                    <h5 class="title">Recent 5 Delivery Documents</h5>
                                                    <div class="content">
                                                        <div class="table-responsive">
                                                            <table class="table table-striped">
                                                                <thead>
                                                                    <tr>
                                                                        <th class="text-center">Id</th>
                                                                        <th>Dealer Name</th>
                                                                        <th>Date</th>
                                                                    </tr>
                                                                    <tbody>
                                                                        <% recentFivedeliveryDocuments.forEach(function(recentFivedeliveryDocument) { %>
                                                                            <tr>
                                                                                <td><a href="/stock/deliveryDocumentContent?deliveryDocumentId=<%= recentFivedeliveryDocument.id %>"><%= recentFivedeliveryDocument.id %></a></td>
                                                                                <td><%= recentFivedeliveryDocument.dealer_name %></td>
                                                                                <td><%= recentFivedeliveryDocument.date %></td>
                                                                            </tr>
                                                                        <% }) %>
                                                                    </tbody>
                                                                </thead>
                                                            </table>
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="col-md-12">
                                                    <div class="row">
                                                        <div class="header">
                                                            Stock Summary
                                                        </div>
                                                        <div class="col-md-12">
                                                            <div class="col-md-6">
                                                                <h5 class="title">Total Stock</h5>
                                                                <div class="content">
                                                                    <div class="table-responsive">
                                                                        <table class="table table-striped">
                                                                            <thead>
                                                                                <tr>
                                                                                    <th>Model</th>
                                                                                    <th>Quantity</th>
                                                                                </tr>
                                                                            </thead>
                                                                            <tbody>
                                                                                <% totalStocks.forEach(function(totalStock) { %>
                                                                                    <tr>
                                                                                        <td><%= totalStock.model_name %></td>
                                                                                        <td><%= totalStock.stock %></td>
                                                                                    </tr>
                                                                                <% }) %>
                                                                            </tbody>
                                                                        </table>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                            <div class="col-md-6">
                                                                <h5 class="title">Yard Stock</h5>
                                                                <div class="content">
                                                                    <div class="table-responsive">
                                                                        <table class="table table-striped">
                                                                            <thead>
                                                                                <tr>
                                                                                    <th>Model</th>
                                                                                    <th>Quantity</th>
                                                                                </tr>
                                                                            </thead>
                                                                            <tbody>
                                                                                <% yardStocks.forEach(function(totalStock) { %>
                                                                                    <tr>
                                                                                        <td><%= totalStock.model_name %></td>
                                                                                        <td><%= totalStock.stock %></td>
                                                                                    </tr>
                                                                                <% }) %>
                                                                            </tbody>
                                                                        </table>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                        <div class="col-md-12">
                                                            <div class="col-md-6">
                                                                <h5 class="title">Showroom Stock</h5>
                                                                <div class="content">
                                                                    <div class="content">
                                                                        <div class="table-responsive">
                                                                            <table class="table table-striped">
                                                                                <thead>
                                                                                    <tr>
                                                                                        <th>Model</th>
                                                                                        <th>Quantity</th>
                                                                                    </tr>
                                                                                </thead>
                                                                                <tbody>
                                                                                    <% showroomStocks.forEach(function(totalStock) { %>
                                                                                        <tr>
                                                                                            <td><%= totalStock.model_name %></td>
                                                                                            <td><%= totalStock.stock %></td>
                                                                                        </tr>
                                                                                    <% }) %>
                                                                                </tbody>
                                                                            </table>
                                                                        </div>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                            <div class="col-md-6">
                                                                <h5 class="title">Dealer Stock</h5>
                                                                <div class="content">
                                                                    <div class="content">
                                                                        <div class="table-responsive">
                                                                            <table class="table table-striped">
                                                                                <thead>
                                                                                    <tr>
                                                                                        <th>Model</th>
                                                                                        <th>Quantity</th>
                                                                                    </tr>
                                                                                </thead>
                                                                                <tbody>
                                                                                    <% dealerStocks.forEach(function(totalStock) { %>
                                                                                        <tr>
                                                                                            <td><%= totalStock.model_name %></td>
                                                                                            <td><%= totalStock.stock %></td>
                                                                                        </tr>
                                                                                    <% }) %>
                                                                                </tbody>
                                                                            </table>
                                                                        </div>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>

                            <% } %>



                        </div>



                    </div>
                </div>

                <% include ./layouts/pro/footer %>
            </div>
        </div>
    </body>
    <% include ./layouts/pro/includes %>
</html>
